
@{
    ViewBag.Title = "Contact";
    ViewBag.a = "";
    ViewBag.b = "";
    ViewBag.c = "active";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<!-- Breadcrumbs -->
<section class="bg-gray-7">
    <div class="breadcrumbs-custom box-transform-wrap context-dark">
        <div class="container">
            <h3 class="breadcrumbs-custom-title">Contacts</h3>
            <div class="breadcrumbs-custom-decor"></div>
        </div>
        <div class="box-transform" style="background-image: url(/assets/images/contactPhoto.jpg);"></div>
    </div>
    <div class="container">
        <ul class="breadcrumbs-custom-path">
            <li><a href="index.html">Home</a></li>
            <li class="active">Contacts</li>
        </ul>
    </div>
</section>
<!-- Contacts-->
<section class="section section-lg bg-default text-md-left">
    <div class="container">
        <div class="row row-60 justify-content-center">
            <div class="col-lg-8">
                
                
                @{Html.RenderAction("Message", "Contacts");}

            </div>
            <div class="col-lg-4">
               
                @{Html.RenderAction("ContactPartial", "home");}

            </div>
        </div>
    </div>
</section>